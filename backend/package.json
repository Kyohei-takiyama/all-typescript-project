{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:debug": "nodemon -L --inspect=0.0.0.0:9229 --env-file=.env dist/index.js",
    "build": "tsc -p .",
    "build:watch": "npm run build --watch",
    "dev": "nodemon --watch './src/**/*.ts' --exec ts-node --files ./src/index.ts --env-file=.env",
    "dev:debug": "npm run build:watch & npm run start:debug --env-file=.env",
    "migrate": "npx prisma migrate dev",
    "migrate:init": "npx prisma migrate dev --name init",
    "check-types": "tsc --noEmit",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix src/**/*.ts",
    "fix:prettier": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/uuid": "^10.0.0",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "prisma": "^5.15.1"
  }
}
